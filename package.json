{
  "publisher": "publisher-slug",
  "name": "ext-slug",
  "displayName": "ext-slug",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.6.2",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/publisher-slug/ext-slug#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/publisher-slug/ext-slug"
  },
  "bugs": {
    "url": "https://github.com/publisher-slug/ext-slug/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.70.0"
  },
  "activationEvents": [
    "onCommand:publisher-slug.preview"
  ],
  "contributes": {
    "commands": [
      {
        "title": "Open Sample Webview",
        "command": "publisher-slug.preview"
      }
    ]
  },
  "scripts": {
    "build": "nr ui:build && rollup -c",
    "dev": "nr build --watch",
    "ui:dev": "nuxt dev ui",
    "ui:build": "nuxt build ui",
    "lint": "eslint .",
    "prepare": "nuxi prepare ui",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit && pnpm --filter ui typecheck",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.16.0",
    "@antfu/ni": "23.3.1",
    "@types/d3": "7.4.3",
    "@types/node": "22.13.10",
    "@types/vscode": "1.98.0",
    "@vscode/vsce": "3.2.2",
    "bumpp": "10.0.3",
    "d3": "7.9.0",
    "d3-flame-graph": "4.1.3",
    "eslint": "9.22.0",
    "open": "10.1.0",
    "rollup": "4.35.0",
    "rollup-plugin-esbuild": "6.2.1",
    "rollup-plugin-vue": "6.0.0",
    "typescript": "5.8.2",
    "vitest": "3.0.8",
    "vue-tsc": "2.2.8"
  }
}
